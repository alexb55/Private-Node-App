    "createsend-node": "https://github.com/alexb55/createsend-node",
    "cron": "^1.3.0",
    "cross-env": "^5.2.0",
    "dateformat": "^3.0.3",
    "debug": "~2.6.3",
    "dotenv": "^4.0.0",
    "ejs": "~2.5.6",
    "express": "~4.17.1",
    "express-mailer": "^0.3.1",
    "express-session": "^1.15.3",
    "fast-xml-parser": "^3.16.0",
    "googleapis": "^42.0.0",
    "history": "^4.7.2",
    "knex": "^0.21.0",
    "morgan": "~1.8.1",
    "n": "^6.5.1",
