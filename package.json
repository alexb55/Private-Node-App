{
  "name": "private-node-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prod": "cross-env NODE_ENV=production yarn run start",
    "dev": "cross-env NODE_ENV=development yarn run start",
    "start": "node ./bin/www",
    "stop": "node ./bin/www",
    "build": "cross-env NODE_ENV=production webpack -p --mode production --config ./config/webpack.config.js --progress --profile --colors",
    "clean": "rm -rf ./assets && mkdir ./assets",
    "pretty": "prettier --single-quote --trailing-comma es5 --write {client,frontend,bin,config,server}{/*,/**/*}.js",
    "precommit": "yarn run build && yarn run pretty",
    "job": "cross-env NODE_ENV=production node ./server/tasks",
    "jobdev": "cross-env NODE_ENV=development node ./server/tasks",
    "cron": "cross-env NODE_ENV=production node ./server/cron"
  },
  "engines": {
    "node": "12.16.2"
  },
  "browsers": [
    "last 3 chrome versions",
    "last 3 firefox versions",
    "last 2 versions",
    "safari >= 8",
    "ios >= 8",
    "ie >= 11",
    "explorermobile >= 11",
    "android >= 4.4"
  ],
  "dependencies": {
    "@shopify/app-bridge": "^1.19.0",
